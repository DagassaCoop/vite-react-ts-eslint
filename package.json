    "bootstrap": "^5.3.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.3",
    "react-bootstrap-icons": "^1.11.4",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.23.1",
    "sass": "^1.77.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.4",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-bootstrap": "^0.26.6",
